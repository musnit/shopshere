{
  "name": "shopsphere",
  "version": "0.0.1",
  "description": "A platform for creating 3D photosphere shops.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "rm -rf dist; NODE_ENV=production webpack --config webpack.config.prod.js; cp prodIndex.html dist/index.html; cp -r public/* dist;",
    "build:widget": "rm -rf dist; NODE_ENV=production webpack --config webpack.config.widget.js; cp widgetIndex.html dist/index.html; mkdir dist/public; mkdir dist/public/images; cp -r public/images/*_hs* dist/public/images;",
    "build": "npm run clean && npm run build:webpack",
    "dev": "node devServer.js",
    "start": "node prodServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Aidan Musnitzky <musnit@gmail.com> (http://github.com/musnit)",
  "license": "CC0-1.0",
